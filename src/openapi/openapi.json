  "components": {
    "schemas": {
      "ExamCheckRecord": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "userId": {
            "type": "string"
          },
          "candidateName": {
            "type": "string"
          },
          "paymentStatus": {
            "type": "string",
            "enum": [
              "PAID",
              "FAILED",
              "PENDING"
            ]
          },
          "checkStatus": {
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "lastUpdated": {
            "type": "string",
            "format": "date-time"
          },
          "checkLimit": {
            "type": "integer",
            "format": "int32"
          },
          "externalRef": {
            "type": "string"
          },
          "waecCandidateEntity": {
            "$ref": "#/components/schemas/WaecCandidateEntity"
          }
        }
      },
      "WaecCandidateEntity": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "cindex": {
            "type": "string"
          },
          "cname": {
            "type": "string"
          },
          "dob": {
            "type": "string"
          },
          "gender": {
            "type": "integer",
            "format": "int32"
          },
          "examyear": {
            "type": "string"
          },
          "examtype": {
            "type": "integer",
            "format": "int64"
          },
          "resultDetails": {
            "type": "array",
